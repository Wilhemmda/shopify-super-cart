// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.sqlite"
}

model Session {
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?
}
model themeId {
  id Int @id @default(autoincrement())
  shop             String
  theme_id              BigInt
  contentCartDrawer String
}
model superCart {
  id Int @id @default(autoincrement())
  title             String
  option1            String
  form               String
  option2            String
  total             String
  option3           String
  footer            String
}
